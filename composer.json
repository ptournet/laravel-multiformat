{
    "name": "ptournet/laravel-multiformat",
    "description": "Multiformat Endpoints in Laravel",
    "keywords": [
        "ptournet",
        "laravel",
        "multiformat"
    ],
    "homepage": "https://github.com/ptournet/laravel-multiformat",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Tournet",
            "email": "ptournet@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "laravel/framework": ">=5.6"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Ptournet\\Multiformat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ptournet\\Multiformat\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ptournet\\Multiformat\\MultiformatServiceProvider"
            ]
        }
    }
}
